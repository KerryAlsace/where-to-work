<% if current_user %>
  Logged in as <%= current_user.username %> 
  <%= link_to 'My Places', user_places_path(current_user) %> 
  <%= link_to 'Create Place', new_user_place_path(current_user) %> 
  <%= link_to 'Log Out', logout_path %>
<% else %>
  <%= link_to 'Log In', login_path %> 
  <%= link_to 'Register', new_user_path %>
<% end %>