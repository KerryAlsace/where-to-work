<% if current_user %>
  Logged in as <%= current_user.username %> 
  <%= link_to 'View All My Places', user_places_path(current_user) %> 
  <%= link_to 'Make a New Place', new_user_place_path(current_user) %> 
  <%= link_to 'Log Out', logout_path %>
<% else %>
  <%= link_to 'Sign In', login_path %> 
  <%= link_to 'Sign Up', new_user_path %>
<% end %>